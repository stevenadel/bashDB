function data_type_validate {
valid=-1

case $2 in
    *int )
        if [[ $1 =~ ^[+-]?[0-9]+$ ]]; then
            valid=1
        fi
        ;;
    *string )
        if [[ $1 =~ ^[a-zA-Z0-9]+$ ]]; then
            valid=1
        fi
        ;;
    *float )
        if [[ $1 =~ ^[+-]?[0-9]+\.?[0-9]*$ ]]; then
            valid=1
        fi
        ;;
    * )
        valid=0
        ;;
esac

return $valid
}
